        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".listas.Lista_de_itens"
            android:label="@string/title_activity_lista_de_itens1"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".listas.Lista_itens_finalizados" />
